{
  "plugins": ["jest"],
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "no-restricted-syntax": 0,
    "max-classes-per-file": 0,
    "no-await-in-loop" : 0
  }
}
